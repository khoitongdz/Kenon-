local webhookRelayURL = "https://discord.com/api/webhooks/1336566970463555675/bxljnPAj4PvekzWmVcz4CQ3wXocakH8FpfQMTjUL8ZEgfT9_xu6n0vr_RC3x7G3RwT3o" -- Thay bằng URL của WebhookRelay
local HttpService = game:GetService("HttpService")

-- Hàm gửi webhook thông qua WebhookRelay
local function sendWebhook(message)
    local data = {
        content = message,
        username = "Kenon Hub Notification|bloxkid",
        avatar_url = "https://img3.thuthuatphanmem.vn/uploads/2019/06/13/anh-nen-anime-cho-may-tinh-dep_095239016.jpg"
    }

    local jsonData = HttpService:JSONEncode(data)
    local success, err = pcall(function()
        HttpService:PostAsync(webhookRelayURL, jsonData, Enum.HttpContentType.ApplicationJson)
    end)

    if success then
        print("Webhook gửi thành công!")
    else
        warn("Lỗi gửi webhook: " .. tostring(err))
    end
end
